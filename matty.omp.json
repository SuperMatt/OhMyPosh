{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "text",
            "style": "plain",
            "foreground": "white",
            "properties": {
              "prefix": "",
              "text": "[",
              "postfix": ""
            }
          },
          {
            "type": "session",
            "style": "plain",
            "foreground": "#56B6C2",
            "properties": {
              "prefix": "",
              "user_color": "cyan",
              "host_color": "red",
              "postfix": ""
            }
          },
          {
            "type": "path",
            "style": "plain",
            "foreground": "yellow",
            "properties": {
              "prefix": " ",
              "style": "agnoster_full",
              "postfix": ""
            }
          },
          {
            "type": "text",
            "style": "plain",
            "foreground": "white",
            "properties": {
              "prefix": "",
              "text": "]",
              "postfix": ""
            }
          },
          {
            "type": "git",
            "style": "plain",
            "foreground": "green",
            "properties": {
              "status_colors_enabled": true,
              "display_status": true,
              "prefix": "<white> (</>",
              "postfix": "<white>)</>"
            }
          },
          {
            "type": "text",
            "style": "plain",
            "foreground": "white",
            "properties": {
              "prefix": " ",
              "text": "$",
              "postfix": ""
            }
          }
        ]
      }
    ],
    "final_space": true
  }
  